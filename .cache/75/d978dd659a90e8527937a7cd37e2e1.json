ends Component {\r\n  constructor () {\r\n    super()\r\n    this.handleClick = this.handleClick.bind(this)\r\n  }\r\n\r\n  handleClick () {\r\n    console.log(\"Hello world\");\r\n  }\r\n  render () {\r\n    return (\r\n    <div>\r\n      <Greetings \r\n        isLoggedIn={false}\r\n      />\r\n      <button onClick={this.handleClick}>\r\n        log\r\n      </button>\r\n    </div>\r\n    )\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"b53e3935a812d3281f6172a4866352a3","cacheData":{"env":{}}}